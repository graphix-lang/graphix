type LineSet = [
  `Normal,
  `Rounded,
  `Double,
  `Thick
];

type LineGauge = {
  filled_style: &[Style, null],
  label: &[Line, null],
  line_set: &[LineSet, null],
  ratio: &f64,
  style: &[Style, null],
  unfilled_style: &[Style, null]
};

val line_gauge: fn(
  ?#filled_style: &[Style, null],
  ?#label: &[Line, null],
  ?#line_set: &[LineSet, null],
  ?#style: &[Style, null],
  ?#unfilled_style: &[Style, null],
  &f64
) -> Tui;
