let write = |path, value| 'write;
let subscribe = |path| 'subscribe;
let call = |path, args| 'call;
let rpc = |#path, #doc, #spec, #f| 'publish_rpc;
let list = |#update = time::timer(1, true), path| 'list;
let list_table = |#clock = time::timer(1, true), path| 'list_table;
let publish = |#on_write = |v| never(v), path, v| 'publish
