{
  "name": "tree-sitter-graphix",
  "version": "1.0.0",
  "description": "Tree-sitter grammar for Graphix programming language",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "tree-sitter",
    "graphix",
    "parser"
  ],
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^8.3.1",
    "node-gyp-build": "^4.8.4"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.24.7"
  },
  "scripts": {
    "generate": "tree-sitter generate",
    "test": "tree-sitter test",
    "build": "tree-sitter generate && tree-sitter build",
    "parse": "tree-sitter parse"
  },
  "tree-sitter": [
    {
      "scope": "source.graphix",
      "file-types": [
        "gx"
      ],
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm",
      "indents": "queries/indents.scm"
    }
  ]
}
